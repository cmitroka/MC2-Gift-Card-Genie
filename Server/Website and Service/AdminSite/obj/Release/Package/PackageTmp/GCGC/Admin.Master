                            <asp:MenuItem NavigateUrl="~/AllUUIDsWVersion.aspx" Text="Show All Downloaders w/ Version" />
                        </asp:MenuItem>
                        <asp:MenuItem NavigateUrl="../GCGWebWS.asmx" Text="Web Service"/>
